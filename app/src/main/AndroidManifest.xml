        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="domain.company.vaisabrina.ListaAlunosActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="domain.company.vaisabrina.FormularioActivity" />
        <activity android:name="domain.company.vaisabrina.FamigeradaActivity" />
    </application>

</manifest>